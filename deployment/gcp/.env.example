# R2R GCP Deployment Environment Variables Template
# Copy this to .env and fill in your actual values

# Google Cloud Project
GCP_PROJECT_ID=your-project-id
GCP_ZONE=us-central1-a
VM_NAME=r2r-production
MACHINE_TYPE=n1-standard-4

# Vertex AI Configuration
VERTEX_PROJECT=your-project-id
VERTEX_LOCATION=us-central1
VERTEXAI_PROJECT=your-project-id
VERTEXAI_LOCATION=us-central1

# API Keys (DO NOT COMMIT THESE)
OPENAI_API_KEY=sk-...your-openai-key
GEMINI_API_KEY=AIza...your-gemini-key
GROQ_API_KEY=gsk_...your-groq-key

# PostgreSQL Configuration
R2R_POSTGRES_USER=postgres
R2R_POSTGRES_PASSWORD=your-secure-password
R2R_POSTGRES_HOST=postgres
R2R_POSTGRES_PORT=5432
R2R_POSTGRES_DBNAME=postgres

# R2R Configuration
R2R_PORT=7272
R2R_HOST=0.0.0.0
R2R_LOG_LEVEL=INFO
R2R_CONFIG_PATH=/app/user_configs/gemini-openai.toml
R2R_PROJECT_NAME=r2r_gcp_production

# Service Account (create via: gcloud iam service-accounts keys create)
GOOGLE_APPLICATION_CREDENTIALS=/app/vertex-ai-key.json
