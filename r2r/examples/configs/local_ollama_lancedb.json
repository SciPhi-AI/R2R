{
    "language_model": {
      "provider": "litellm"
    },
    "vector_database": {
      "provider": "lancedb",
      "collection_name": "demo_vecs"
    },
    "ingestion":{
      "provider": "local"
    },
    "embedding": {
      "provider": "sentence-transformers",
      "search_model": "mixedbread-ai/mxbai-embed-large-v1",
      "search_dimension": 512,
      "batch_size": 32,
      "text_splitter": {
        "type": "recursive_character",
        "chunk_size": 512,
        "chunk_overlap": 20
      }
    },
    "evals": {
      "provider": "parea",
      "frequency": 0.0
    },
    "app": {
      "max_logs": 100,
      "max_file_size_in_mb": 100
    },
    "logging_database": {
      "provider": "local",
      "collection_name": "demo_logs",
      "level": "INFO"
    }
  }