{
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next"
      },
      {
        "src": "docs/package.json",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/docs/(.*)",
        "dest": "/docs/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }